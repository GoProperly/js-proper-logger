{
  "name": "@goproperly/js-proper-logger",
  "description": "Properly's backend logger for JavaScript",
  "license": "UNLICENSED",
  "author": "Properly Inc",
  "version": "1.1.0",
  "main": "dist/js-proper-logger.cjs.js",
  "module": "dist/js-proper-logger.esm.js",
  "types": "dist/js-proper-logger.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "commit": "git-cz",
    "dev": "rollup -c -w",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . || true",
    "release": "semantic-release",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoProperly/js-proper-logger.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@goproperly/eslint-config-properly-base": "^1.2.15",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.12",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^1.4.12",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "semantic-release": "^17.2.3",
    "ts-node": "^7.0.1",
    "tslib": "^1.13.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json,md,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  }
}
