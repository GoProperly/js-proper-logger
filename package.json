{
  "name": "@goproperly/js-proper-logger",
  "description": "Properly's backend logger for JavaScript",
  "license": "UNLICENSED",
  "author": "Properly Inc",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoProperly/js-proper-logger.git"
  },
  "version": "0.0.1",
  "main": "dist/js-proper-logger.cjs.js",
  "module": "dist/js-proper-logger.esm.js",
  "scripts": {
    "build": "rollup -c",
    "commit": "git-cz",
    "dev": "rollup -c -w",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . || true",
    "release": "semantic-release",
    "test": "echo \"No tests yet!\" && exit 0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^4.0.2",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@goproperly/eslint-config-properly-base": "^1.2.9",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "2.1.1",
    "semantic-release": "^17.1.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  }
}
